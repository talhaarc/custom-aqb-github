                            }
                        }
                    },
                    "422": {
                        "description": "Validation Error",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/HTTPValidationError"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/removeCollab": {
            "delete": {
                "summary": "Removecollaborator",
                "operationId": "removeCollaborator_removeCollab_delete",
                "parameters": [
                    {
                        "name": "reponame",
                        "in": "query",
                        "required": true,
                        "schema": {
                            "type": "string",
                            "title": "Reponame"
                        }
                    },
                    {
                        "name": "logging_user",
                        "in": "query",
                        "required": true,
                        "schema": {
                            "type": "string",
